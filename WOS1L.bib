@article{ WOS:000518549700007,
Author = {Yamada, Hiroshi and Takeda, Masahiro},
Title = {{Report of the Study Group on data and competition policy in Japan}},
Journal = {{INTERNATIONAL DATA PRIVACY LAW}},
Year = {{2019}},
Volume = {{9}},
Number = {{4}},
Pages = {{299-301}},
Month = {{NOV}},
Abstract = {{The Japan Fair Trade Commission (JFTC) published its study group report
   on data and competition policy in June 2017.
   The Competition Policy Research Center (CPRC) considered that it was
   necessary to draw a distinction between personal data and industrial
   data to initiate the analysis.
   The study group discussed a basic approach to collection and utilization
   of data, potential anticompetitive conduct and factors to be considered
   in merger review.
   The CPRC concluded that the current framework under the Antimonopoly Act
   is applicable to most competition concerns about the collection and
   utilization of data.
   However, there remain issues not yet addressed, such as `digital
   cartels'. JFTC should pay attention to these issues that may require
   further consideration.}},
